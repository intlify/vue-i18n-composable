{
  "name": "vue-i18n-composable",
  "version": "0.2.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/intlify/vue-i18n-composable",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup src/index.ts --format cjs,esm --dts --external vue,vue-i18n,@vue/composition-api",
    "dev": "npm run build --watch",
    "release": "npx bumpp --commit --tag && npm publish && git push"
  },
  "peerDependencies": {
    "vue": ">= 2.5",
    "vue-i18n": ">= 8",
    "@vue/composition-api": ">= 1.0.0-beta.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.3.3",
    "@vue/composition-api": "^1.0.0-beta.22",
    "eslint": "^7.9.0",
    "tsup": "^3.6.1",
    "typescript": "^4.0.2",
    "vue": "^2.6.12",
    "vue-i18n": "^8.21.1"
  }
}
